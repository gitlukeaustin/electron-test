      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "gitlukeaustin",
              "name": "electron-test-example"
            },
            "draft": true
          }
        }
      ]
    }
  },
  "keywords": [],
